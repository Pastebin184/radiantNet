{
    "format_version":"1.16.0",
    "minecraft:entity":{
        "description":{
            "identifier":"minecraft:player",
            "is_spawnable":false,
            "is_summonable":false,
            "is_experimental":false
        },
        "components": {
            "minecraft:damage_sensor":{
                "triggers": [
                   { 
                        "on_damage":{
                            "filters":{
                                "any_of":[
                                    {
                                        "all_of": [
                                            { "test":"has_tag", "value":"onSpawn" },
                                            { "test":"has_tag", "subject":"other", "value":"onSpawn" }
                                        ]
                                    }
                                ]
                            }
                        },
                        "deals_damage": false 
                    }
                ]
            }
        }
    }
}